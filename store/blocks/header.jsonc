{
  "header": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header-layout.desktop": {
    "children": ["header-row#1-desktop"]
  },
  "header-row#1-desktop": {
    "children": [
      "header-border",
      "search-bar",
      "header-spacer",
      "vtex.menu@2.x:menu#header",
      "logo#desktop",
      //"header-spacer",
      "minicart.v2"
    ],
    "props": {
      "fullWidth": true,

      "inverted": true,
      "blockClass": "headerDesktop",
      "sticky": true
    }
  },
  "logo#desktop": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/Logo-Corebiz.png",
      "width": "180"
    }
  },
  "header-layout.mobile": {
    "children": ["header-row#1-mobile"]
  },
  "header-row#1-mobile": {
    "children": [
      "drawer",
      "search-bar",
      "header-spacer",
      "logo#mobile",
      "minicart.v2"
    ],
    "props": {
      "fullWidth": true,
      "inverted": true,
      "blockClass": "headerMobile",
      "sticky": true
    }
  },
  "logo#mobile": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/Logo-Corebiz-Reduced.png",
      "width": "50"
    }
  },
  "vtex.menu@2.x:menu#header": {
    "props": {
      "blockClass": "menu-desktop"
    },
    "children": [
      "menu-item#shop",
      "menu-item#about-us",
      "menu-item#leads"
    ]
  },
  "drawer": {
    "children": ["menu#drawer"]
  },
  "menu#drawer": {
    "children": [
      "menu-item#shop",
      "menu-item#about-us",
      "menu-item#leads"
    ],
    "props": {
      "orientation": "vertical"
    }
  },
  "menu-item#shop": {
    "props": {
      "id": "menu-item-shop",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/shop",
        "noFollow": false,
        "tagTitle": "Shop",
        "text": "produtos"
      }
    }
  },
  "menu-item#about-us": {
    "props": {
      "id": "menu-item-about-us",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/about-us",
        "noFollow": false,
        "tagTitle": "about-us",
        "text": "sobre n√≥s"
      }
    }
  },
  
  "menu-item#leads": {
    "props": {
      "id": "menu-item-leads",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/leads-client",
        "noFollow": false,
        "tagTitle": "Leads",
        "text": "clientes"
      }
    }
  },
  "search-bar": {
    "props": {
      "width": 50
    }
  }
}
