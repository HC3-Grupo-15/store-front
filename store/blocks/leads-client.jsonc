{
    "store.custom#leads-client": {
        "blocks": [
            //"flex-layout.row#leads"
             "responsive-layout.desktop#leads-desktop",
             "responsive-layout.mobile#leads-mobile",
             "leads-list"    
        ]
        
    },


    "responsive-layout.desktop#leads-desktop":{
        "children":[
          "flex-layout.row#leads"
          
        ]
      },
    
      "responsive-layout.mobile#leads-mobile": {
        "children": [
            "flex-layout.row#leads"
        ]
      },

      "flex-layout.row#leads": {
        "children": [
            "image#leads",
            "rich-text#title1"
           
        ],
        "props":{
          "paddingTop": 7,
          "blockClass":"layout-home"
        }
      },

      "image#leads":{
        "props": {
          "src":"assets/vans-shop.jpg",               
          "maxHeight": 400,
          "title":"corebiz - vans",
          "blockClass":"image-leads",
          "sizes": {
            "phone": "16:9"
          }
        }
        
      },
      

      "rich-text#title1":{
        "props": { 
            "text": "consulte aqui nossos clientes!",
            "textAlignment": "CENTER",
            "textPosition": "CENTER",
            "font": "t-heading-1",
            "blockClass":"title-leads"
          }

      }



}